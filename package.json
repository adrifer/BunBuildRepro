  "scripts": {
    "start": "AUTH_COOKIE_SECURE=false bun run src/server.ts",
    "start:prod": "NODE_ENV=production bun run dist/server/server.js",
    "build:client": "NODE_ENV=production bunx --bun vite build --outDir dist/client",
    "build:server": "NODE_ENV=production bunx --bun vite build --outDir dist/server --ssr src/server.ts",
    "build": "bun run build:client && bun run build:server",
    "typecheck": "tsc --noEmit",
    "test": "razzle test --env=jsdom",
    "test:ci": "razzle test --env=jsdom --coverage --watchAll=false --coverageReporters=cobertura --reporters=jest-junit",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prettyAllProject": "prettier --write \"./**/*.{ts,tsx}\"",
    "dev": "AUTH_COOKIE_SECURE=false ts-node-esm server.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
